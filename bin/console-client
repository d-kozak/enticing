#!/bin/bash
ENTICING_HOME=$("$(dirname "$BASH_SOURCE")"/../scripts/utils/get_enticing_home.sh)
retval=$?
if [[ "$retval" -ne "0" ]]; then
  exit $retval
fi
java -cp "${ENTICING_HOME}/lib/console-client-0.0.1-SNAPSHOT.jar:${ENTICING_HOME}/lib/mg4j-bin/*" cz.vutbr.fit.knot.enticing.index.client.MainKt "$@"