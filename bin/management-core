#!/bin/bash
export ENTICING_HOME=$("$(dirname "$BASH_SOURCE")"/../scripts/utils/get_enticing_home.sh)
retval=$?
if [[ "$retval" -ne "0" ]] ; then
  exit $retval
fi
echo "repository found at: $ENTICING_HOME"
java -cp "${ENTICING_HOME}/lib/management-core-0.0.1-SNAPSHOT.jar" cz.vutbr.fit.knot.enticing.management.MainKt "$@"